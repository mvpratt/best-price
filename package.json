{
  "name": "best-price",
  "version": "1.0.0",
  "description": "Find best crypto-asset prices",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "dbinit": "node db/dbInit.js",
    "build1": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build": "webpack -d",
    "build:prod": "webpack -p && cp src/index.html dist/index.html",
    "dev-server": "NODE_ENV=dev-server node server.js",
    "dev-api": "NODE_ENV=dev-api node server.js",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run dev-api\"",
    "prod": "webpack -p && node server.js",
    "clean": "rm -rf dist/app"
  },
  "author": "mvpratt@gmail.com",
  "license": "MIT",
  "engines": {
    "node": "8.4.0"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "moment": "^2.20.1",
    "pg": "^7.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "recharts": "^1.0.0-beta.7",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "whatwg-fetch": "^2.0.3",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "style-loader": "^0.19.1"
  }
}
